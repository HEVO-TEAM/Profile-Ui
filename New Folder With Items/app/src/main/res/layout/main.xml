<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bg_hevo"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/b_hevo_1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingTop="8dp"
			android:paddingRight="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_hevo1"
				android:layout_width="25dp"
				android:layout_height="25dp"
				android:src="@drawable/hevo_team_1"
				android:scaleType="fitXY" />
			<LinearLayout
				android:id="@+id/b_hevo_2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="1" />
			<ImageView
				android:id="@+id/image_hevo2"
				android:layout_width="25dp"
				android:layout_height="25dp"
				android:src="@drawable/hevo_team_8"
				android:scaleType="fitXY" />
			<CheckBox
				android:id="@+id/check_hevo"
				android:layout_width="0dp"
				android:layout_height="0dp"
				android:gravity="center_horizontal|center_vertical"
				android:textSize="12sp"
				android:textColor="#000000"
				android:alpha="0.0"
				android:layout_gravity="center_horizontal|center_vertical" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="200dp"
			android:gravity="center_horizontal"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo4"
					android:layout_width="175dp"
					android:layout_height="175dp"
					android:background="#101D24"
					android:src="@drawable/hevo_team_logo"
					android:scaleType="centerCrop"
					android:layout_gravity="center_horizontal|center_vertical" />
			</LinearLayout>
			<ImageView
				android:id="@+id/image_hevo3"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:padding="10dp"
				android:background="#FF4433"
				android:src="@drawable/hevo_team_5_1"
				android:scaleType="fitCenter"
				android:translationX="60.0dp"
				android:translationY="-50.0dp" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/b_hevo_4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<TextView
				android:id="@+id/text_hevo1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="HEVO TEAM"
				android:textSize="16sp"
				android:textColor="#000000" />
			<TextView
				android:id="@+id/text_hevo2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="hevoteamofficial\@gmail.com"
				android:textSize="12sp"
				android:textColor="#000000" />
			<LinearLayout
				android:id="@+id/bt_hevo_pro"
				android:layout_width="200dp"
				android:layout_height="wrap_content"
				android:layout_marginBottom="10dp"
				android:padding="4dp"
				android:background="#FF4433"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/text_hevo_pro"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Upgrade to PRO"
					android:textSize="14sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/bg_hevo_2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:paddingLeft="4dp"
			android:paddingRight="4dp"
			android:paddingBottom="4dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/bt_hevo_1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo5"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_10"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Privacy"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt1"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bt_hevo_2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo6"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_6"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Purchase History"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt2"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bt_hevo_3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo7"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_5_2"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Help &amp; Support"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt3"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bt_hevo_4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo8"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_7"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Settings"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt4"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bt_hevo_5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo9"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_4"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Invite a Friend"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt5"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/bt_hevo_6"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#F3F7FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/image_hevo10"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginLeft="4dp"
					android:src="@drawable/hevo_team_3"
					android:scaleType="fitXY" />
				<TextView
					android:id="@+id/text_hevo8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Logout"
					android:textSize="12sp"
					android:textColor="#000000"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/image_hevo_bt6"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/hevo_team_2"
					android:scaleType="fitXY" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
